{
  "name": "friendzone",
  "version": "0.0.0",
  "private": true,
  "workspaces": ["packages/*", "projects/*"],
  "scripts": {
    "app": "cd projects/mobile && yarn dev",
    "blog": "turbo run dev --filter blog",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "db": "yarn prisma-format && yarn db-push",
    "db-push": "cd packages/db && yarn db-push",
    "prisma-format": "cd packages/db && prisma format", 
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.10"
}
